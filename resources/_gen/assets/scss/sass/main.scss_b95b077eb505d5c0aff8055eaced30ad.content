*{box-sizing:border-box}body{color:#c3eaff;background:#16212f;background-size:15px 15px;background-image:radial-gradient(#466A97 3%,transparent 4%),radial-gradient(#466A97 3%,transparent 4%);font-family:nova mono,monospace;text-rendering:optimizeLegibility;-webkit-font-smoothing:antialiased;-moz-osx-font-smoothing:grayscale;font-size:13px;margin:0}a{color:#c3eaff;text-decoration:none}a:visited{color:#c3eaff}.main-header{border-top:1px solid #4c5b6d;border-bottom:1px solid #4c5b6d;display:flex;align-items:center}.main-header h1{font-size:medium;flex-grow:1;padding:5px 15px;margin:0}.main-navigation{display:flex;list-style:none;margin:0;padding:0}.main-navigation li{flex-grow:1;padding:5px}.main-navigation a{color:#c3eaff}.list-container{display:block}@media(min-width:768px){.list-container{display:flex;align-content:center;align-items:stretch;flex-wrap:wrap}}.list-title{padding-left:15px}.list-item{border-top:1px solid #4c5b6d;border-right:1px solid #4c5b6d;padding:15px}.list-item h4{margin:0;border-bottom:1px solid #4c5b6d}.list-item .intro{color:#4c5b6d}.list-item .item-tag{background:#c3eaff;color:#4c5b6d;padding:2px 5px;font-size:smaller}.list-item .item-date{margin-bottom:10px;text-transform:uppercase}.list-item .tags h5{margin:0;margin-bottom:5px}@media(min-width:768px){.list-item{width:33%;flex-grow:1}}